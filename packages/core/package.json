{
	"name": "@kheopskit/core",
	"version": "0.0.22",
	"description": "",
	"private": false,
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "tsup --watch",
		"build": "tsup",
		"prepack": "tsup",
		"clean": "rm -rf ./dist && rm -rf ./node_modules",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [],
	"author": "Kheops (https://github.com/0xKheops)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kheopskit/kheopskit.git"
	},
	"license": "ISC",
	"peerDependencies": {
		"polkadot-api": ">=1.21.0",
		"rxjs": ">=7.0.0"
	},
	"dependencies": {
		"@reown/appkit": "^1.8.17",
		"@walletconnect/types": "^2.23.4",
		"@walletconnect/universal-provider": "^2.23.4",
		"@walletconnect/utils": "^2.23.4",
		"lodash": "^4.17.23",
		"mipd": "^0.0.7",
		"viem": "^2.45.1"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.23",
		"polkadot-api": "^1.23.3",
		"rxjs": "^7.8.2"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"tsconfig": "tsconfig.build.json",
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020"
	}
}
