  "license": "ISC",
  "peerDependencies": {
    "polkadot-api": ">=1.10.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@reown/appkit": "^1.7.6",
    "@walletconnect/types": "^2.20.3",
